<%= form_for(@image, :url => admin_images_path, :html => { :multipart => true } ) do |f| %>
<div class="form-group">
  <%= f.label :title %>
  <%= f.text_field :title, :class => 'form-control' %>
</div>

<div class="form-group">
  <%= f.label :upfile %>
  <%= f.file_field :upfile, :class => 'form-control' %>
</div>

<div class="actions">
  <%= f.submit 'submit', :class => 'btn btn-primary' %>
</div>
<%- end %>
