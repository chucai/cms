<h3>Listing navigations</h3>

<table class='table'>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @navigations.each do |navigation| %>
  <tr>
    <td><%= navigation.name %></td>
    <td><%= navigation.url %></td>
    <td><%= link_to 'Show', navigation %></td>
    <td><%= link_to 'Edit', edit_navigation_path(navigation) %></td>
    <td><%= link_to 'Destroy', navigation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Navigation', new_navigation_path %>
